utm dynamic:
	import action straight<location[]>
operation basic:
	start: navigation_point
	drones: drone[]
	a: drone
	c: location[]
implementation
	drones += straight(start)
	
	a += straight(start)
	for b in c using d from drones
		for aa in c using z from drones
			d += straight(b)
			z += straight(aa)
		end
	end


operation second:
	drones: drone[]
	start: navigation_point
	implementation
		drones += straight(start)
