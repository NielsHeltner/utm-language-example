utm dynamic:
	import action straight<location[]>
mission basic:
	start: navigation_point
	drones: drone[]
	a: drone
	c: location[]
implementation
	drones += straight(start)
	
	a += straight(start)
	
	a += straight(start)
	for b in c using d from drones
		for aa in c using z from drones
			for bb in c
				z += straight(b)
			end
		end
	end
	
	a += straight(start)

constraints

